{
  "name": "cxengage-configuration-ui",
  "version": "0.64.0",
  "description": "CxEngage Configuration and Reporting",
  "repository": {
    "type": "git",
    "url": "https://github.com/SerenovaLLC/cxengage-configuration-ui"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write --print-width 120",
      "git add"
    ]
  },
  "publishConfig": {
    "registry": "http://nexus.cxengagelabs.net/content/repositories/npm-internal/"
  },
  "scripts": {
    "clearCache:jest": "jest --clearCache",
    "start": "webpack-dev-server --port 3000 --mode development",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "profile": "webpack --profile --json > stats.json",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "npm run test -- --coverage",
    "test:auto": "node ./node_modules/cx-automation-utils | wdio ./automation/wdio.conf.js --suite \"inProgress\"",
    "test:all": "wdio ./automation/wdio.conf.js --suite \"inProgress\"",
    "test:preMerge": "wdio ./automation/wdio.conf.js --suite \"users\"",
    "lint": "eslint src/",
    "precommit": "npm run lint && lint-staged && npm run test:coverage",
    "serve": "http-server ./build -p 3000"
  },
  "author": "SerenovaLLC",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-create-automation-page-objects": "^2.0.0",
    "core-js": "^2.6.9",
    "css-loader": "0.28.11",
    "cx-automation-utils": "^1.1.2",
    "cx-js-sdk": "^1.0.1",
    "cx-ui-components": "^0.50.0",
    "draft-js": "^0.10.5",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.3",
    "eslint": "4.16.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.42.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.6.0",
    "html-webpack-plugin": "3.0.7",
    "http-server": "^0.11.1",
    "husky": "0.14.3",
    "immutable": "^3.8.2",
    "jest": "^24.5.0",
    "jest-junit": "^6.3.0",
    "lint-staged": "6.1.0",
    "moment": "2.22.0",
    "postcss-loader": "2.1.3",
    "prettier": "1.10.2",
    "prop-types": "15.6.1",
    "react": "^16.4.1",
    "react-beautiful-dnd": "^10.0.3",
    "react-color": "^2.14.1",
    "react-copy-to-clipboard": "5.0.1",
    "react-dom": "16.4.1",
    "react-intl": "^2.4.0",
    "react-json-view": "^1.19.1",
    "react-moment": "0.7.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-table": "6.7.6",
    "react-test-renderer": "16.4.1",
    "redux": "3.7.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-form": "7.2.3",
    "redux-immutable": "4.0.0",
    "redux-observable": "0.18.0",
    "reselect": "3.0.1",
    "rxjs": "5.5.7",
    "sanitize.css": "^7.0.1",
    "serenova-js-utils": "^1.6.18",
    "style-loader": "0.20.3",
    "styled-components": "3.1.4",
    "url-loader": "1.0.1",
    "wdio-jasmine-framework": "^0.3.6",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.13.2",
    "webpack": "4.2.0",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "2.0.13",
    "webpack-dev-server": "3.1.1"
  }
}
