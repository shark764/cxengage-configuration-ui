{
  "name": "cxengage-configuration-ui",
  "version": "0.12.0",
  "description": "CxEngage Configuration and Reporting",
  "repository": {
    "type": "git",
    "url": "https://github.com/SerenovaLLC/cxengage-configuration-ui"
  },
  "private": true,
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --port 3000 --mode development",
    "build": "webpack --mode production",
    "profile": "webpack --profile --json > stats.json",
    "test": "react-scripts test --coverage --env=jsdom",
    "test:update": "react-scripts test --coverage --env=jsdom --updateSnapshot",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/containers/**/*.{js,jsx}",
      "src/redux/modules/*.{js}",
      "!<rootDir>/node_modules/"
    ]
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "author": "SerenovaLLC",
  "license": "UNLICENSED",
  "dependencies": {
    "cx-ui-components": "0.12.0",
    "immutable": "3.8.2",
    "moment": "2.20.1",
    "react": "16.2.0",
    "react-copy-to-clipboard": "5.0.1",
    "react-dom": "16.2.0",
    "react-intl": "2.4.0",
    "react-moment": "0.7.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-table": "6.7.6",
    "redux": "3.7.2",
    "redux-form": "7.2.3",
    "redux-immutable": "4.0.0",
    "redux-observable": "0.18.0",
    "reselect": "3.0.1",
    "rxjs": "5.5.7",
    "styled-components": "3.1.4"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "html-webpack-plugin": "^3.1.0",
    "husky": "0.14.3",
    "lint-staged": "6.1.0",
    "prettier": "1.10.2",
    "prop-types": "15.6.1",
    "react-test-renderer": "16.2.0",
    "webpack": "4.2.0",
    "webpack-cli": "2.0.13",
    "webpack-dev-server": "3.1.1"
  }
}
